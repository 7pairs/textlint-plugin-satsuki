{
  "name": "textlint-plugin-satsuki",
  "version": "1.0.0",
  "description": "Satsuki notation support for textlint",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "prepublish": "npm run build",
    "test": "mocha",
    "coveralls": "mocha test --require blanket --reporter mocha-lcov-reporter | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7pairs/textlint-plugin-satsuki.git"
  },
  "keywords": [
    "textlint",
    "plugin",
    "satsuki"
  ],
  "author": "Jun-ya HASEBA",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/7pairs/textlint-plugin-satsuki/issues"
  },
  "homepage": "https://github.com/7pairs/textlint-plugin-satsuki",
  "config": {
    "blanket": {
      "pattern": [
        "textlint-plugin-satsuki/src"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "dependencies": {
    "txt-to-ast": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.7.2",
    "blanket": "^1.2.3",
    "coveralls": "^2.11.9",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "power-assert": "^1.3.1",
    "textlint": "^6.3.0"
  }
}
